package day17_Word;

import java.util.ArrayList;
import java.util.List;

public class Word {
	// word, List<String> mean
	private String word;
	private List<String> mean = new ArrayList<>();
	
	public Word(String word) {
		this.word = word;
	}
	public Word(String word, List<String>mean) {
		this.word = word;
		this.mean = mean;
	}
	
}
