package day16;

import java.util.HashMap;
import java.util.Map;

public class Lambda02 {

	public static void main(String[] args) {
		
		Map<String, Integer> map = new HashMap<>();
		map.put("hong" , 89);
		map.put("kim" , 92);
		map.put("lee" , 91);
		map.put("park" , 90);
		map.put("yoon" , 89);
		
	}

}
